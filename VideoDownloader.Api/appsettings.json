{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information",
        "System.Net.Http.HttpClient": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
        }
      }
    ],
    "Enrich": [ "WithExceptionDetails" ]
  },
  "ApiOptions": {
    "VideoEditingUrl": "https://www.openshot.org/cloud-api/",
    "VideoDownloadUrl": "https://youtube.com/",
    "CheckForHttps": true
  }
}
